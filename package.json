{
  "name": "hooks-project",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "build": "preconstruct build",
    "dev": "preconstruct dev",
    "watch": "preconstruct watch",
    "test": "jest --passWithNoTests",
    "cov": "yarn dev && yarn test --coverage",
    "ci": "yarn cov --ci",
    "format": "pretty-quick",
    "publish-local": "lerna exec yalc publish",
    "push-local": "lerna exec yalc push",
    "publish-release": "yarn build && changeset publish",
    "publish-next": "yarn build && lerna publish --dist-tag=next",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.18.6",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@changesets/cli": "2.25.0",
    "@jest/expect": "28.1.3",
    "@preconstruct/cli": "2.2.2",
    "@types/jest": "28.1.8",
    "execa": "4.1.0",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "jest-serializer-path": "0.1.15",
    "jest-snapshot-serializer-raw": "1.2.0",
    "jest-watch-typeahead": "1.1.0",
    "lerna": "latest",
    "patch-package": "6.4.7",
    "prettier": "2.8.1",
    "pretty-quick": "3.1.3",
    "typescript": "4.8.4",
    "yalc": "1.0.0-pre.53"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "all-contributors-cli": "6.20.5"
  }
}
