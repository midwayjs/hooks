{
  "name": "hooks-project",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "build": "preconstruct build",
    "dev": "preconstruct dev",
    "test": "jest --passWithNoTests",
    "cov": "yarn dev && yarn test --coverage",
    "ci": "yarn cov --ci",
    "format": "pretty-quick",
    "publish-local": "lerna exec yalc publish",
    "publish-release": "yarn build && lerna publish",
    "publish-next": "yarn build && lerna publish --dist-tag=next",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "7.16.7",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/plugin-proposal-private-methods": "7.16.7",
    "@babel/plugin-proposal-private-property-in-object": "7.16.7",
    "@babel/preset-env": "7.16.8",
    "@babel/preset-typescript": "7.16.7",
    "@changesets/cli": "2.19.0",
    "@preconstruct/cli": "2.1.5",
    "@types/jest": "27.4.0",
    "execa": "4.1.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "jest-serializer-path": "0.1.15",
    "jest-snapshot-serializer-raw": "1.2.0",
    "jest-watch-typeahead": "1.0.0",
    "lerna": "latest",
    "patch-package": "6.4.7",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "typescript": "4.5.4",
    "yalc": "1.0.0-pre.53"
  },
  "workspaces": [
    "packages/*",
    "examples/fullstack/*",
    "examples/api/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "resolutions": {
    "@midwayjs/core": "3.0.0-beta.16",
    "@midwayjs/decorator": "3.0.0-beta.16",
    "@midwayjs/mock": "3.0.0-beta.16",
    "@midwayjs/koa": "3.0.0-beta.16",
    "@midwayjs/bootstrap": "3.0.0-beta.16",
    "@midwayjs/serverless-app": "3.0.0-beta.16"
  },
  "devDependencies": {
    "all-contributors-cli": "6.20.0"
  }
}
